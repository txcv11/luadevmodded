local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/massthix/testttt/refs/heads/main/newmenu'))()

local pastebinURL = "https://pastebin.com/raw/cRRWdZy3" -- Kendi Pastebin raw linkini koy

local player = game.Players.LocalPlayer
local userID = player.UserId
local AllowedKeys = {}
local expiryTime = 0 -- GLOBAL olarak tanƒ±mlandƒ±

-- Pastebin'den keyleri √ßek
local success, result = pcall(function()
    return game:HttpGet(pastebinURL)
end)

if success and result then
    for line in result:gmatch("[^\r\n]+") do
        local id, key, duration, startTime = line:match("(%d+):(.+):(%d+):(%d+)")
        if id and key and duration and startTime then
            local tempExpiryTime = tonumber(startTime) + tonumber(duration) -- Ge√ßici expiry
            local currentTime = os.time() -- Mevcut zaman

            if tempExpiryTime > currentTime then
                AllowedKeys[tonumber(id)] = key
                if tonumber(id) == userID then
                    expiryTime = tempExpiryTime -- Kullanƒ±cƒ±ya ait expiryTime
                end
            end
        end
    end
else
    warn("An error occurred while fetching the keys from Pastebin.")
end

local UserKey = AllowedKeys[userID]

if UserKey then
    print("Success! Player ID " .. userID .. " Valid key for: " .. UserKey)
else
    print("The key has expired or is invalid! Player ID" .. userID .. " There is no valid key for")
end

local KeySystemEnabled = UserKey ~= nil

if KeySystemEnabled then
    local Window = Rayfield:CreateWindow({
        Name = "SPTS MODDED PremiumXüëø",
        Icon = "atom",
        LoadingTitle = "PremiumXüëø",
        LoadingSubtitle = "By LU4DEV",
        Theme = "Dark",
        DisableRayfieldPrompts = false,
        DisableBuildWarnings = false,

        ConfigurationSaving = {
            Enabled = true,
            FolderName = nil,
            FileName = "Big Hub"
        },

        Discord = {
            Enabled = true,
            Invite = "aUdm6zcAhw",
            RememberJoins = false
        },

        KeySystem = KeySystemEnabled,
        KeySettings = {
            Title = "Keys",
            Subtitle = "Key System",
            Note = "The key is personal and sharing is prohibited. Users who share it will permanently lose their premium status.",
            FileName = "Key",
            SaveKey = false,
            GrabKeyFromSite = false,
            Key = UserKey and {UserKey} or {"INVALID"}
        }
    })

    -- üìå **Key S√ºresi Sayacƒ± i√ßin Label**
    local KeyTab = Window:CreateTab("Key", 0)
    local KeySection = KeyTab:CreateSection("Key Status")
    local TimeLabel = KeyTab:CreateLabel("Key Duration: Calculating...", "clock", Color3.fromRGB(50, 50, 50), false)

    -- ‚è≥ **Saya√ß Fonksiyonu**
    task.spawn(function()
        while KeySystemEnabled do
            local currentTime = os.time()
            local remainingTime = expiryTime - currentTime

            if remainingTime <= 0 then
                TimeLabel:Set("Key has expired! ‚ùå")
                break
            else
                local days = math.floor(remainingTime / 86400)
                local hours = math.floor((remainingTime % 86400) / 3600)
                local minutes = math.floor((remainingTime % 3600) / 60)
                local seconds = remainingTime % 60

                local timeText = string.format("Key Duration: %d Days, %d Hours, %d Minutes, %d Seconds", days, hours, minutes, seconds)
                TimeLabel:Set(timeText)
            end

            wait(1) -- Her saniye g√ºncellensin
        end
    end)

    -- Diƒüer Tablar
    local MainTab = Window:CreateTab("üëëMain", 0)
    local FistTab = Window:CreateTab("Fist", 0)
    local BodyTab = Window:CreateTab("Body", 0)
    local PsychicTab = Window:CreateTab("Psychic", 0)
    local MainSection = MainTab:CreateSection("Main")
    local FistSection = FistTab:CreateSection("Fist")
    local BodySection = BodyTab:CreateSection("Body")
    local PsychicSection = PsychicTab:CreateSection("Psychic")

    local MainTab = Window:CreateTab("Just click StackX once.", "atom") -- Title, Image

